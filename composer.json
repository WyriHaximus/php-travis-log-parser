{
    "name": "wyrihaximus/travis-log-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "reactivex/rxphp": "^1.5"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Travis\\LogParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Travis\\LogParser\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
